#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tro/Live/miniCool/react/node_modules/.pnpm/@antfu+eslint-config@2.12.2_@vue+compiler-sfc@3.4.21_eslint@8.56.0_typescript@5.4.4/node_modules/@antfu/eslint-config/bin/node_modules:/home/tro/Live/miniCool/react/node_modules/.pnpm/@antfu+eslint-config@2.12.2_@vue+compiler-sfc@3.4.21_eslint@8.56.0_typescript@5.4.4/node_modules/@antfu/eslint-config/node_modules:/home/tro/Live/miniCool/react/node_modules/.pnpm/@antfu+eslint-config@2.12.2_@vue+compiler-sfc@3.4.21_eslint@8.56.0_typescript@5.4.4/node_modules/@antfu/node_modules:/home/tro/Live/miniCool/react/node_modules/.pnpm/@antfu+eslint-config@2.12.2_@vue+compiler-sfc@3.4.21_eslint@8.56.0_typescript@5.4.4/node_modules:/home/tro/Live/miniCool/react/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tro/Live/miniCool/react/node_modules/.pnpm/@antfu+eslint-config@2.12.2_@vue+compiler-sfc@3.4.21_eslint@8.56.0_typescript@5.4.4/node_modules/@antfu/eslint-config/bin/node_modules:/home/tro/Live/miniCool/react/node_modules/.pnpm/@antfu+eslint-config@2.12.2_@vue+compiler-sfc@3.4.21_eslint@8.56.0_typescript@5.4.4/node_modules/@antfu/eslint-config/node_modules:/home/tro/Live/miniCool/react/node_modules/.pnpm/@antfu+eslint-config@2.12.2_@vue+compiler-sfc@3.4.21_eslint@8.56.0_typescript@5.4.4/node_modules/@antfu/node_modules:/home/tro/Live/miniCool/react/node_modules/.pnpm/@antfu+eslint-config@2.12.2_@vue+compiler-sfc@3.4.21_eslint@8.56.0_typescript@5.4.4/node_modules:/home/tro/Live/miniCool/react/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/index.js" "$@"
else
  exec node  "$basedir/../../bin/index.js" "$@"
fi
